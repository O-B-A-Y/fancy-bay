{
  "name": "fancy-bay",
  "description": "OBAY client of fanciness",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "bietdoikiem",
    "email": "minhlucky2408@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/O-B-A-Y/fancy-bay.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "next:lint": "next lint",
    "lint": "eslint \"**/*.+(js|jsx|ts|tsx)\"",
    "lint:fix": "eslint --fix \"**/*.+(js|jsx|ts|tsx)\"",
    "prettier": "prettier --check \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "prettier:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|css)\"",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "gat": "concurrently \"npm:gat-*\"",
    "gat-token": "abi-types-generator ./src/contracts/ERC20.json --output=./src/types",
    "gat-dao": "abi-types-generator ./src/contracts/TreasureBay.json --output=./src/types",
    "gat-dao-factory": "abi-types-generator ./src/contracts/TreasureBayFactory.json --output=./src/types",
    "gat-transfer-proposal": "abi-types-generator ./src/contracts/TransferProposal.json --output=./src/types"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ],
    "*.{json,md,css}": "yarn prettier:fix"
  },
  "dependencies": {
    "@ethersproject/address": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/constants": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@ethersproject/solidity": "^5.5.0",
    "@ethersproject/units": "^5.5.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@sushiswap/core": "^2.0.0-canary.1",
    "@sushiswap/core-sdk": "1.0.0-canary.1",
    "@sushiswap/default-token-list": "16.18.0",
    "@sushiswap/limit-order-pair-list": "0.2.2",
    "@sushiswap/sdk": "5.0.0-canary.96",
    "@web3-react/core": "^6.1.9",
    "@web3-react/frame-connector": "^6.0.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.8",
    "aos": "^2.3.4",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.1",
    "bn.js": "^5.2.0",
    "clsx": "^1.1.1",
    "concurrently": "^6.4.0",
    "currency-symbol-map": "^5.0.1",
    "currency.js": "^2.0.4",
    "eip-712": "^0.4.3",
    "framer-motion": "^4",
    "moment": "^2.29.1",
    "next": "12.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropdown": "^1.9.2",
    "react-dropzone": "^11.4.2",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^4.0.0",
    "react-modal": "^3.14.3",
    "react-redux": "^7.2.6",
    "react-table": "^7.7.0",
    "react-toastify": "^8.1.0",
    "react-tooltip": "^4.2.21",
    "redux-logger": "^3.0.6",
    "sass": "^1.43.4",
    "web3": "^1.6.0",
    "web3-react": "^5.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/aos": "^3.0.4",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-modal": "^3.13.1",
    "@types/redux-logger": "^3.0.9",
    "@types/web3": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "7",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-next": "12.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "ethereum-abi-types-generator": "^1.3.2",
    "ethers": "^5.5.1",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1",
    "stylelint-config-standard-scss": "^2.0.0",
    "stylelint-scss": "^4.0.0",
    "typescript": "4.4.4"
  }
}
